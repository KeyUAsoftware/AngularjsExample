    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.11.0"
  },
  "dependencies": {
    "angular": "^1.5.0-beta.0",
    "angular-ui-bootstrap": "^2.5.0",
    "angular-ui-router": "^1.0.3"
